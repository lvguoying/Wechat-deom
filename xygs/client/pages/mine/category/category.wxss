/* pages/mine/category/category.wxss */
#category{
  padding-bottom: 100rpx;
  font-size: 30rpx;
}
.cate-title{
  background-color: #ccc;
  color: #fff;
  text-align: center;
}
.cate-cont{
  width: 640rpx;
  margin: 0 auto;
}
.wrapper{
  width: 320rpx;
  height: 200rpx;
  margin: 20rpx 0;
  float: left;
}
.box{
  background-color: #fff;
  width: 200rpx;
  height: 200rpx;
  border: 2rpx solid #ccc;
  border-radius: 10%;
  text-align: center;
  margin: 0 auto;
}
.box view{
  display: inline-block;
}
.cate-img{
  width: 64rpx;
  height: 64rpx;
  margin: 25rpx;
}
/*隐藏滚动条  */
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}