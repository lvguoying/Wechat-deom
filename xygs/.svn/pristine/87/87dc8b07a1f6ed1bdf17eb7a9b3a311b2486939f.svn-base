}
.add{
  width: 90%;
  position: absolute;
  bottom: 20rpx;
  height: 70rpx;
  line-height: 70rpx;
  left: 50%;
  transform: translateX(-50%);
  border-radius: 20rpx;
  background: #1AAD19;
  font-size: 32rpx;
}