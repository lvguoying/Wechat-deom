// pages/help/helpmore/helpmore.js
Page({

  /**
   * 页面的初始数据
   */
  data: {
    bol:true,
    bool:false
  },
  closeok: function() {
    var bol=this.data.bol;
    var bool = this.data.bool;
    if (bool == true) {
      this.setData({
        bool: false
      })
    }
    if (bol == false) {
      this.setData({
        bol: true
      })
    }
  },
  closeno:function(){
    var bol = this.data.bol;
    var bool = this.data.bool;
    if(bool==false){
      this.setData({
        bool:true
      })
    }
    if(bol==true){
      this.setData({
        bol:false
      })
    }
    
    
  },
  /**
