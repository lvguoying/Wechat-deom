<!--pages/free/free.wxml-->
<view class='searchbox'>
  <input class='search' placeholder='请输入规格，交货地，品牌' placeholder-style='color:#fff;font-size:28rpx' disabled='true' bindtap='tosearch'></input>
</view>
<view class='box'>
<swiper class='swiper' indicator-dots="true" autoplay="true" interval="5000" duration="1000" circular="true">
<block wx:for="{{swiper}}" wx:for-index="index">    
        <swiper-item>    
          <image src="{{item.url}}" class="slide-image" mode="aspectFill"/>    
        </swiper-item>    
      </block>    
</swiper>
<view class='header' bindtap='toOrder'>
<image class='ordericon' src='../../mock/images/icon/index_999.png'></image>
</view>
</view> 
<view class='bigChoose'>
  <view class='kind'>
   <picker bindchange="bindPickerChange" value="{{index}}" range="{{array}}">
    <image src='../../mock/images/icon/index_51.png' class='bigicon'></image>
  </picker>
  </view>
  <view class='area'>
<picker mode="region" bindchange="bindRegionChange" value="{{region}}" custom-item="{{customItem}}">
    <image src='../../mock/images/icon/index_54.png' class='bigicon'></image>
  </picker>
  </view> 
</view>


 
