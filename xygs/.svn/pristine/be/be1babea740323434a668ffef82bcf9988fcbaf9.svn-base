/* pages/mine/priService/priService.wxss*/
.myButton{
  background-color: #1195db;
  color: #fff;
  width: 100%;
  position: absolute;
  bottom: 0;
}
scroll-view{
  height: 1200rpx;
}
/*隐藏滚动条  */
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.pri image{
  width: 100%;
  display: block;
}
.pri-box{
  position: relative;
}
.pri-title{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  color: #1195db;
  font-size: 36px;
}