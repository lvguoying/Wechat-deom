<view class="header">
  <view class="header-back" bindtap="headerBack">X</view>
  <view class="logo"><image src="../../../mock/images/head.jpg" mode="scaleToFill"></image></view>
</view>
<form>
  <view class="phone">
    <view class="phone-num">
      <input name="phone-num" placeholder="手机号" />
      <button size="mini">获取验证码</button>
    </view>
    <view class="code">
      <input name="code" placeholder="验证码" />
    </view>
  </view>
      <!-- <input value="获取验证码" /></view>  -->
  
  <view class="btn">
     <button>注册</button>
    <button>登录</button> 
  </view>
</form>
