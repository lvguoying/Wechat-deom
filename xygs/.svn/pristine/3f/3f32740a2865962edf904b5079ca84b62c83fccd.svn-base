/* pages/free/free.wxss */
.header{
  height: 74rpx;
  text-align: center;
  line-height: 74rpx;
  
}
.ordername{
  font-size: 64rpx;
  font-weight: 400;
}
.swiper{
  height: 260rpx;
  width: 100%;
  
}
.swiper image {
  height: 100%;
  width: 100%;
  position: relative;
}
.bigChoose{
  height: 124rpx;
  display: flex;  
}
.kind,.area{
  flex-grow:1; 
  font-size: 40rpx;
  border: 1px solid #000;
}
.smallChoose{
  display: flex;
  text-align: center;
}
.recommand,.immediate,.history,.sell,.buy{
  flex-grow:1; 
  width: 84rpx;
}
.smalltext{
  width: 84rpx;
  display: inline-block;
}
.icon{
  width: 84rpx;
  height: 84rpx;
}
.message{
  border-top:15rpx solid #eee;
  height: 135rpx
}
.sbkind{
  height: 100rpx;
  
}
.iconsellbuy{
  width: 51rpx;
  height: 52rpx;
  line-height: 52rpx;
  margin: 21rpx;
  float: left;
}
.meskind{
  font-size: 28rpx;
  height: 100rpx;
  line-height: 100rpx;
  display: inline-block; 
}
